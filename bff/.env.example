# === Conductores BFF – Environment (keys PENDING) ===

# App
PORT=3000
PWA_URL=http://localhost:4200

# Database (optional in dev; if unset, BFF uses in-memory store)
NEON_DATABASE_URL=postgres://USER:PASS@HOST:5432/DBNAME

# S3 (presign) – leave keys pending for now
AWS_ACCESS_KEY_ID=__PENDING__
AWS_SECRET_ACCESS_KEY=__PENDING__
AWS_REGION=us-east-1
AWS_S3_BUCKET=__PENDING__

# Optional: direct upload URL override (for non-standard endpoints)
# S3_UPLOAD_URL=https://s3.us-east-1.amazonaws.com/your-bucket

# OpenAI (OCR/Vision or Whisper) – leave key pending
OPENAI_API_KEY=__PENDING__

# Logging
LOG_LEVEL=info

